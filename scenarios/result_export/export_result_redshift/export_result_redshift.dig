timezone: UTC

_export:
    endpoint: api.treasuredata.com
    database: sample_datasets

+select_data:
  td>: queries/sample.sql

+export_to_redshift:
  http>: https://${endpoint}/v3/job/result_export/${td.last_job.id}
  method: POST
  content_type: application/json
  content: 
    result: 'redshift://${secret:redshiftUser}:${secret:redshiftPass}@${secret:redshiftHost}:${secret:redshiftPort}/${secret:redshiftDatabase}/${secret:redshiftTable}?mode=replace'

+teardown:
  echo>: finish ${session_time}